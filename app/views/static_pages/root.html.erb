<script type="text/javascript">
  <% if logged_in? %>
    window.currentUser = <%= render(
      'api/users/user.json', user: current_user
    ).html_safe %>
  <% end %>
  window.images = {
    caret: "<%= image_path("caret.png") %>",
    caretSelected: "<%= image_path("caret-selected.png") %>",
    play: "<%= image_path("play.png") %>",
    playSmall: "<%= image_path("play-small.png") %>",
    pause: "<%= image_path("pause.png") %>"
  }
</script>

<main id="root"></main>
